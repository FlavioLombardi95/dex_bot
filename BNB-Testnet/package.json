{
  "name": "bsc-triangular-arbitrage-bot",
  "version": "2.0.0",
  "description": "Bot per arbitraggio triangolare su BSC Testnet con PancakeSwap - Senza flash loans",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start:prod": "NODE_ENV=production node app.js",
    "start:dev": "NODE_ENV=development node app.js",
    "start:testnet": "NETWORK=bscTestnet node app.js",
    "start:mainnet": "NETWORK=bscMainnet node app.js",
    "start:triangular": "node scanners/triangular-scanner.js",
    "start:realtime": "node realtime-new-tokens-scanner.js",
    "start:profit": "node real-profit-scanner.js",
    "test": "npx hardhat test",
    "test:bsc": "npx hardhat test --network bscTestnet",
    "test:triangular": "node scripts/test-triangular-opportunities.js",
    "test:config": "node -e \"console.log(require('./core/config').getConfig())\"",
    "test:connection": "node -e \"require('./core/connection-pool').getStats().then(console.log)\"",
    "test:health": "curl -s http://localhost:3000/api/health | jq",
    "test:full": "npm run test:config && npm run test:connection && npm run test:health",
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat run scripts/deploy.js",
    "deploy:optimized": "npx hardhat run scripts/deploy-optimized.js",
    "deploy:bsc": "npx hardhat run scripts/deploy.js --network bscTestnet",
    "deploy:mainnet": "npx hardhat run scripts/deploy.js --network bscMainnet",
    "verify": "npx hardhat verify",
    "verify:bsc": "npx hardhat verify --network bscTestnet",
    "setup": "node scripts/setup.js",
    "setup:optimized": "node scripts/setup-optimized.js",
    "setup:bsc": "node scripts/setup-bsc.js",
    "config": "node scripts/final-configuration.js",
    "analyze": "node scripts/arbitrage-opportunity-analyzer.js",
    "config:show": "node -e \"console.log(JSON.stringify(require('./core/config').getConfig(), null, 2))\"",
    "config:validate": "node -e \"require('./core/config'); console.log('âœ… Configuration valid')\"",
    "analyze:costs": "node scripts/complete-cost-analysis.js",
    "analyze:triangular": "node scripts/test-triangular-opportunities.js",
    "analyze:real": "node scripts/real-data-analyzer.js",
    "scan:triangular": "node scanners/triangular-scanner.js",
    "scan:realtime": "node realtime-new-tokens-scanner.js",
    "scan:profit": "node real-profit-scanner.js",
    "api:start": "ENABLE_API=true node app.js",
    "api:health": "curl -s http://localhost:3000/api/health",
    "api:scanners": "curl -s http://localhost:3000/api/scanners",
    "api:opportunities": "curl -s http://localhost:3000/api/opportunities",
    "logs": "tail -f logs/main.log",
    "logs:error": "tail -f logs/errors.log",
    "logs:opportunities": "tail -f logs/opportunities.log",
    "logs:profits": "tail -f logs/profits.log",
    "logs:analytics": "tail -f logs/analytics.log",
    "logs:clean": "rm -rf logs/*.log",
    "db:reset": "rm -f data/arbitrage.db",
    "db:migrate": "node -e \"require('./app').createTables()\"",
    "cache:clear": "node -e \"require('./core/connection-pool').resetStats()\"",
    "health": "node -e \"require('./app').performHealthCheck()\"",
    "stats": "node -e \"require('./app').getPerformanceReport().then(console.log)\"",
    "clean": "rm -rf artifacts cache node_modules",
    "clean:logs": "rm -rf logs/*.log",
    "clean:data": "rm -rf data/*.db",
    "clean:all": "npm run clean && npm run clean:logs && npm run clean:data",
    "install:deps": "npm install express cors express-rate-limit sqlite3 sqlite redis",
    "test:basic": "node test-basic.js",
    "test:scanner": "node test-scanner.js",
    "test:simple": "npm run test:basic && npm run test:scanner"
  },
  "keywords": [
    "crypto",
    "arbitrage",
    "triangular",
    "pancakeswap",
    "bsc",
    "binance-smart-chain",
    "solidity"
  ],
  "author": "BSC Triangular Arbitrage Bot",
  "license": "MIT",
  "dependencies": {
    "@balancer-labs/v2-interfaces": "^0.4.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@openzeppelin/contracts": "^5.3.0",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ethers": "^5.8.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "node-cron": "^3.0.2",
    "redis": "^4.6.0",
    "sqlite": "^4.2.1",
    "sqlite3": "^5.1.6",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "chai": "^4.3.7",
    "hardhat": "^2.25.0",
    "mocha": "^10.2.0"
  }
}
